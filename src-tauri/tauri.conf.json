{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Canda",
  "version": "0.1.0",
  "identifier": "com.canda.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "",
        "label": "main",
        "width": 840,
        "height": 500,
        "transparent": true,
        "decorations": false,
        "titleBarStyle": "Transparent",
        "resizable": false,
        "hiddenTitle": true,
        "shadow": false
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
